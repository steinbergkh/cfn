{
  "name": "cfn",
  "version": "0.17.0",
  "description": "Simple Cloud Formation for Node.js",
  "main": "index.js",
  "scripts": {
    "test": "standard && mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nordstrom/cfn.git"
  },
  "files": [
    "index.js",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "xo": {
    "space": 4,
    "rules": {
      "max-lines": [
        "warn",
        400
      ],
      "babel/object-curly-spacing": [
        "error",
        "always"
      ],
      "one-var": "off",
      "operator-linebreak": [
        "error",
        "before"
      ],
      "no-use-extend-native/no-use-extend-native": "off"
    }
  },
  "keywords": [
    "Cloud",
    "Formation",
    "CloudFormation",
    "Cloud-Formation",
    "Node.js",
    "Stack",
    "Deploy"
  ],
  "author": "Andy Day",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Nordstrom/cfn/issues"
  },
  "homepage": "https://github.com/Nordstrom/cfn#readme",
  "dependencies": {
    "aws-sdk": "^2.7.17",
    "bluebird": "^3.4.1",
    "chalk": "^1.1.3",
    "https-proxy-agent": "^1.0.0",
    "lodash": "^4.13.1",
    "moment": "^2.13.0",
    "sprintf": "^0.1.5"
  },
  "devDependencies": {
    "aws-sdk-mock": "^1.6.1",
    "coveralls": "^2.11.15",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.2.0",
    "node-uuid": "^1.4.7",
    "should": "^11.1.2",
    "should-sinon": "0.0.5",
    "sinon": "^1.17.7",
    "standard": "^8.6.0"
  }
}
